{
    "name": "sxbrsky/http-message",
    "description": "A strict PSR-7 HTTP message implementation.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "psr-7",
        "request",
        "response",
        "message",
        "http",
        "http-message"
    ],
    "authors": [
        {
            "name": "Dominik Szamburski",
            "email": "dominikszamburski99@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "11.4.x-dev"
    },
    "provide": {
        "psr/http-message-implementation": "2.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sxbrsky\\HttpMessage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sxbrsky\\HttpMessage\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
